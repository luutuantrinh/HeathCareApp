<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LikesActivity">

    <include android:id="@+id/include_toolbar_likes" layout="@layout/toolbar_likes"></include>

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_below="@id/include_toolbar_likes"
        android:id="@+id/rcy_likes"
        android:layout_height="wrap_content">

    </androidx.recyclerview.widget.RecyclerView>
    <include android:id="@+id/include_bottom_bar_likes" layout="@layout/bottom_navigation_bar"></include>

</RelativeLayout>