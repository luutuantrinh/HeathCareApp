<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center" android:id="@id/circleColor">
        <shape android:shape="oval">
            <solid android:color="@color/primary" />
            <stroke android:width="2.0dip" android:color="@color/transparent" />
        </shape>
    </item>
    <item android:gravity="center" android:id="@id/border">
        <shape android:shape="oval">
            <stroke android:width="2.0dip" android:color="@color/white" />
        </shape>
    </item>
</layer-list>